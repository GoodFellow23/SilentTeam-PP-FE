{
  "extends": [
    "plugin:react/recommended",
    "plugin:import/react",
    "prettier",
    "airbnb-base"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": { "requireConfigFile": false },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "comma-dangle": "off",
    "no-console": 0,
    "react/prop-types": "off",
    "@typescript-eslint/comma-dangle": "off",
    "semi": ["error", "always"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
